<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" minHeight="448.0" minWidth="500.0" prefHeight="463.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane fx:id="resultsStackPane" layoutY="40.0" prefHeight="230.0" prefWidth="500.0" style="-fx-background-color: black;" />
      <Button fx:id="searchButton" layoutX="425.0" layoutY="5.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="75.0" style="-fx-border-radius: 0px;" text="Search">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <TextField fx:id="searchTextField" layoutX="1.0" layoutY="5.0" prefHeight="30.0" prefWidth="423.0" />
      <StackPane layoutY="270.0" style="-fx-background-color: linear-gradient(#686868 0%, #232723 25%, #373837 75%, #757575 100%),        linear-gradient(#020b02, #3a3a3a),        linear-gradient(#9d9e9d 0%, #6b6a6b 20%, #343534 80%, #242424 100%),        linear-gradient(#8a8a8a 0%, #6b6a6b 20%, #343534 80%, #262626 100%),        linear-gradient(#777777 0%, #606060 50%, #505250 51%, #2a2b2a 100%);">
         <children>
            <Label fx:id="totalResultsLabel" alignment="CENTER" prefHeight="17.0" prefWidth="500.0" style="-fx-text-fill: white; -fx-effect: dropshadow( one-pass-box , black , 0, 0.0 , 0 , -1 );" text="Showing 0-10 Of 248 Results">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </StackPane>
      <Label fx:id="selectedTitleLabel" layoutX="14.0" layoutY="330.0" text="Selected: Batman Begins">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="selectedYearLabel" layoutX="14.0" layoutY="350.0" text="Year: 2005">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="viewFullDefinitionButton" layoutY="372.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="500.0" text="View Full Definition">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="setAsDefinitionButton" layoutY="402.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="500.0" text="Set As Definition">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutY="432.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="500.0" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="398.0" layoutY="300.0" minHeight="17.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="102.0" text="Next Page &gt;" />
      <Button layoutY="300.0" minHeight="17.0" mnemonicParsing="false" text="&lt; Previous Page" />
   </children>
</AnchorPane>
